{
	"name": "feathericons.com",
	"license": "MIT",
	"scripts": {
		"build-dataset": "ts-node src/data/build.ts > src/data/raw.json",
		"prerender-duomo-css": "sass --load-path=node_modules --no-source-map duomo.config.scss build/stylesheets/duomo.css",
		"prerender-css": "sass --load-path=node_modules --no-source-map src/stylesheets/index.scss build/stylesheets/style.css",
		"watch-css": "nodemon --exec 'yarn prerender-css' --ext scss --watch src/stylesheets",
		"test": "jest",
		"start": "rm -rf build && mkdir build && cp src/index.html build && cp -r public build && yarn concurrently 'yarn prerender-duomo-css' 'yarn prerender-css' && node scripts/start.js",
		"restart": "node scripts/start.js",
		"build": "rm -rf build && mkdir build && cp src/index.html build && cp -r public build && yarn concurrently 'yarn prerender-duomo-css' 'yarn prerender-css' && NODE_ENV=production node scripts/build.js"
	},
	"dependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-feather": "^2.0.8",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@mdx-js/mdx": "^1.6.22",
		"@mdx-js/react": "^1.6.22",
		"@types/mdx-js__react": "^1.5.3",
		"@types/node": "^14.14.14",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.6",
		"@types/sass": "^1.16.0",
		"chokidar": "^3.4.3",
		"concurrently": "^5.3.0",
		"esbuild": "^0.8.25",
		"express": "^4.17.1",
		"jest": "^26.6.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"sass": "^1.30.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	}
}
