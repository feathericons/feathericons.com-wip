{
	"name": "feathericons.com",
	"license": "MIT",
	"scripts": {
		"prebuild-dataset": "yarn --silent ts-node src/data/build.ts > src/data/raw.json",
		"prerender-css": "node scripts/prerender-css.js",
		"prerender": "node scripts/prerender.js && node src/server/prerender.js && rm src/server/prerender.js",
		"test": "jest",
		"start": "rm -rf build && mkdir build && yarn prerender node scripts/start.js",
		"build": "rm -rf build && mkdir build && yarn prerender NODE_ENV=production node scripts/build.js",
		"serve": "live-server --host=localhost --port=8080 build"
	},
	"dependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-feather": "^2.0.8",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@types/markdown-it": "^12.0.0",
		"@types/node": "^14.14.14",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.6",
		"@types/sass": "^1.16.0",
		"chokidar": "^3.4.3",
		"esbuild": "^0.8.25",
		"jest": "^26.6.3",
		"live-server": "^1.2.1",
		"markdown-it": "^12.0.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"sass": "^1.30.0",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	}
}
